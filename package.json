  "homepage": "https://github.com/hujiulong/gcoord#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "codecov": "^3.0.0",
    "eslint": "^5.7.0",
    "jest": "^23.6.0",
    "rollup": "^0.67.1",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6",
    "uglify-js": "^3.3.9"
